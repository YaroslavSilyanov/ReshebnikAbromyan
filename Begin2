//Дана сторона квадрата a. Найти его площадь S = a2
#include <stdio.h>
 
int main()
{
    int a;
    printf("введите значение стороны a:");
    
    scanf ("%d", &a);
 
    int S= a*a;
    
    printf("площадь квадата равна S:%d",S);
 
    return 0;
}
